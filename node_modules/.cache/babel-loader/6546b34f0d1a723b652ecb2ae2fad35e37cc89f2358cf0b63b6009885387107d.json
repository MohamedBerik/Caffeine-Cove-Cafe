{"ast":null,"code":"import{useEffect,useState}from\"react\";export default function useDebounce(value){let delay=arguments.length>1&&arguments[1]!==undefined?arguments[1]:500;const[debouncedValue,setDebouncedValue]=useState(value);useEffect(()=>{const handler=setTimeout(()=>{setDebouncedValue(value);},delay);return()=>clearTimeout(handler);},[value,delay]);return debouncedValue;}","map":{"version":3,"names":["useEffect","useState","useDebounce","value","delay","arguments","length","undefined","debouncedValue","setDebouncedValue","handler","setTimeout","clearTimeout"],"sources":["C:/React/Caffeine Cove/src/hooks/useDebounce.js"],"sourcesContent":["import { useEffect, useState } from \"react\";\r\n\r\nexport default function useDebounce(value, delay = 500) {\r\n  const [debouncedValue, setDebouncedValue] = useState(value);\r\n\r\n  useEffect(() => {\r\n    const handler = setTimeout(() => {\r\n      setDebouncedValue(value);\r\n    }, delay);\r\n\r\n    return () => clearTimeout(handler);\r\n  }, [value, delay]);\r\n\r\n  return debouncedValue;\r\n}\r\n"],"mappings":"AAAA,OAASA,SAAS,CAAEC,QAAQ,KAAQ,OAAO,CAE3C,cAAe,SAAS,CAAAC,WAAWA,CAACC,KAAK,CAAe,IAAb,CAAAC,KAAK,CAAAC,SAAA,CAAAC,MAAA,IAAAD,SAAA,MAAAE,SAAA,CAAAF,SAAA,IAAG,GAAG,CACpD,KAAM,CAACG,cAAc,CAAEC,iBAAiB,CAAC,CAAGR,QAAQ,CAACE,KAAK,CAAC,CAE3DH,SAAS,CAAC,IAAM,CACd,KAAM,CAAAU,OAAO,CAAGC,UAAU,CAAC,IAAM,CAC/BF,iBAAiB,CAACN,KAAK,CAAC,CAC1B,CAAC,CAAEC,KAAK,CAAC,CAET,MAAO,IAAMQ,YAAY,CAACF,OAAO,CAAC,CACpC,CAAC,CAAE,CAACP,KAAK,CAAEC,KAAK,CAAC,CAAC,CAElB,MAAO,CAAAI,cAAc,CACvB","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}